  "onboarding-1-header": {
    "message": "\u0a24\u0a41\u0a39\u0a3e\u0a21\u0a40\u0a06\u0a02 \u0a38\u0a3e\u0a30\u0a40\u0a06\u0a02 \u0a06\u0a28\u0a32\u0a3e\u0a08\u0a28 \u0a38\u0a30\u0a17\u0a30\u0a2e\u0a40\u0a06\u0a02 \u0a26\u0a47 \u0a07\u0a70\u0a24\u0a5b\u0a3e\u0a2e \u0a26\u0a3e \u0a35\u0a27\u0a40\u0a06 \u0a22\u0a70\u0a17"
  },
  "onboarding-1-description": {
    "message": "\u0a15\u0a28\u0a1f\u0a47\u0a28\u0a30\u0a3e\u0a02 \u0a28\u0a42\u0a70 \u0a15\u0a70\u0a2e \u0a26\u0a3e \u0a2c\u0a70\u0a26\u0a4b\u0a2c\u0a38\u0a24 \u0a15\u0a30\u0a28, \u0a16\u0a3e\u0a24\u0a3f\u0a06\u0a02 \u0a26\u0a3e \u0a2a\u0a4d\u0a30\u0a2c\u0a70\u0a27 \u0a15\u0a30\u0a28 \u0a24\u0a47 \u0a1c\u0a3f\u0a71\u0a25\u0a47 \u0a24\u0a41\u0a38\u0a40\u0a02 \u0a1a\u0a3e\u0a39\u0a4b, \u0a13\u0a25\u0a47 \u0a27\u0a3f\u0a06\u0a28 \u0a15\u0a47\u0a02\u0a26\u0a30\u0a3f\u0a24 \u0a15\u0a30\u0a28 \u0a32\u0a08 \u0a35\u0a30\u0a24\u0a4b\u0a02\u0964"
  },
  "onboarding-2-header": {
    "message": "\u0a15\u0a28\u0a1f\u0a47\u0a28\u0a30\u0a3e\u0a02 \u0a28\u0a42\u0a70 \u0a06\u0a2a\u0a23\u0a47 \u0a32\u0a08 \u0a35\u0a30\u0a24\u0a4b\u0a02\u0964"
