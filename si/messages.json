  },
  "onboarding-1-header": {
    "message": "\u0d94\u0db6 \u0db8\u0dcf\u0dbb\u0dca\u0d9c\u0d9c\u0dad\u0dc0 \u0dc3\u0dd2\u0daf\u0dd4 \u0d9a\u0dbb\u0db1 \u0dc3\u0dd2\u0dba\u0dc5\u0dd4 \u0daf\u0dd1 \u0d9a\u0dc5\u0db8\u0db1\u0dcf\u0d9a\u0dbb\u0dab\u0dba\u0da7 \u0dc4\u0ddc\u0db3 \u0d9a\u0dca\u200d\u0dbb\u0db8\u0dba\u0d9a\u0dca"
  },
  "onboarding-1-description": {
    "message": "\u0d9a\u0dcf\u0dbb\u0dca\u0dba \u0dc3\u0d82\u0dc0\u0dd2\u0db0\u0dcf\u0db1\u0dba\u0da7, \u0d9c\u0dd2\u0dab\u0dd4\u0db8\u0dca \u0d9a\u0dc5\u0db8\u0db1\u0dcf\u0d9a\u0dbb\u0dab\u0dba\u0da7 \u0dc3\u0dc4 \u0d94\u0db6\u0da7 \u0d85\u0dc0\u0dc1\u0dca\u200d\u0dba \u0dad\u0dd0\u0db1\u0d9a\u0da7 \u0d85\u0dc0\u0db0\u0dcf\u0db1\u0dba \u0dba\u0ddc\u0db8\u0dd4 \u0d9a\u0dd2\u0dbb\u0dd3\u0db8\u0da7 \u0d85\u0da9\u0d82\u0d9c\u0dd4 \u0db7\u0dcf\u0dc0\u0dd2\u0dad\u0dcf \u0d9a\u0dbb\u0db1\u0dca\u0db1."
  },
  "onboarding-2-header": {
    "message": "\u0d94\u0db6\u0d9c\u0dda \u0db4\u0dca\u200d\u0dbb\u0dba\u0ddd\u0da2\u0db1\u0dba\u0da7 \u0d85\u0da9\u0d82\u0d9c\u0dd4 \u0dba\u0ddc\u0daf\u0dcf\u0d9c\u0db1\u0dca\u0db1."
  },
  "onboarding-2-description": {
    "message": "\u0dc0\u0dbb\u0dca\u0dab-\u0d9a\u0dda\u0dad\u0db1\u0dba \u0dc3\u0dc4 \u0db7\u0dd2\u0db1\u0dca\u0db1 \u0db4\u0da7\u0dd2\u0dad\u0dd2 \u0d85\u0da9\u0d82\u0d9c\u0dd4 \u0dc0\u0dd0\u0db1\u0dd2 \u0dc0\u0dd2\u0dc1\u0dda\u0dc2\u0dcf\u0d82\u0d9c \u0d94\u0db6\u0da7 \u0db6\u0ddd\u0dc4\u0ddd \u0daf\u0dd1 \u0db4\u0dc4\u0dc3\u0dd4\u0dc0\u0dd9\u0db1\u0dca \u0dc3\u0ddc\u0dba\u0dcf \u0d9c\u0dd0\u0db1\u0dd3\u0db8\u0da7, \u0d9a\u0dcf\u0dbb\u0dca\u0dba\u0dba\u0da7 \u0dc3\u0dd2\u0dad \u0dba\u0ddc\u0db8\u0dd4 \u0d9a\u0dd2\u0dbb\u0dd3\u0db8\u0da7 \u0dc4\u0dcf \u0d85\u0dc0\u0db0\u0dcf\u0db1\u0dba\u0da7 \u0db6\u0dcf\u0db0\u0dcf \u0dc0\u0dd3\u0db8 \u0d85\u0dc0\u0db8 \u0d9a\u0dd2\u0dbb\u0dd3\u0db8\u0da7 \u0d8b\u0db4\u0d9a\u0dcf\u0dbb\u0dd3 \u0dc0\u0dda."
  },
  "onboarding-3-header": {
    "message": "\u0dc3\u0dd1\u0db8 \u0daf\u0dd9\u0dba\u0d9a\u0dca\u0db8 \u0dc3\u0db3\u0dc4\u0dcf \u0dc3\u0dca\u0dae\u0dcf\u0db1\u0dba\u0d9a\u0dca, \u0dc3\u0dc4 \u0dc3\u0dd2\u0dba\u0dbd\u0dca\u0dbd \u0d91\u0db8 \u0dc3\u0dca\u0dae\u0dcf\u0db1\u0dba\u0dd9\u0dc4\u0dd2."
  },
  "onboarding-3-description": {
    "message": "\u0dc3\u0dcf\u0daf\u0dcf \u0dad\u0dd2\u0db6\u0dd9\u0db1 \u0d85\u0da9\u0d82\u0d9c\u0dd4 \u0dc3\u0db8\u0d9f \u0d85\u0dbb\u0db9\u0db1\u0dca\u0db1, \u0dc4\u0ddc\u0dca \u0d94\u0db6\u0db8 \u0dc3\u0dcf\u0daf\u0db1\u0dca\u0db1."
  },
  "onboarding-4-header": {
    "message": "\u0d94\u0db6\u0da7 \u0dc0\u0dd4\u0dc0\u0db8\u0db1\u0dcf \u0d85\u0da9\u0d82\u0d9c\u0dd4\u0dc0\u0dbd \u0dc3\u0dd0\u0db8\u0dc0\u0dd2\u0da7 \u0d85\u0da9\u0dc0\u0dd2 \u0d85\u0dbb\u0dd2\u0db1\u0dca\u0db1."
  },
  "onboarding-4-description": {
    "message": "\u0d85\u0da9\u0dc0\u0dd2\u0dba\u0d9a\u0dca \u0dc3\u0dd0\u0db8\u0dc0\u0dd2\u0da7 \u0d85\u0da9\u0d82\u0d9c\u0dd4\u0dc0 \u0dad\u0dd4\u0dc5 \u0dc0\u0dd2\u0dc0\u0dd8\u0dad \u0dc0\u0dd3\u0db8\u0da7 \u0dc3\u0dd0\u0d9a\u0dc3\u0dd3\u0db8 \u0dc3\u0db3\u0dc4\u0dcf \u0db4\u0da7\u0dd2\u0dad\u0dd2 \u0d85\u0da9\u0d82\u0d9c\u0dd4\u0dc0\u0d9a\u0dca \u0d87\u0dad\u0dd4\u0dc5\u0dad \u0daf\u0d9a\u0dd4\u0dab\u0dd2\u0db1\u0dca \u0d94\u0db6\u0db1\u0dca\u0db1."
  },
  "onboarding-5-header": {
    "message": "\u0d94\u0db6\u0da7 \u0dc0\u0dd4\u0dc0\u0db8\u0db1\u0dcf \u0dc0\u0dd2\u0da7 \u0db4\u0da7\u0dd2\u0dad\u0dd2 \u0d85\u0da9\u0d82\u0d9c\u0dd4\u0dc0\u0d9a\u0da7 \u0daf\u0db8\u0db1\u0dca\u0db1."
  },
  "onboarding-5-description": {
    "message": "\u0db1\u0dc0 \u0db4\u0da7\u0dd2\u0dad\u0dd2 \u0d85\u0da9\u0d82\u0d9c\u0dd4\u0dc0\u0d9a\u0dca \u0dc3\u0dd1\u0daf\u0dd3\u0db8\u0da7 \u0db1\u0dc0 \u0db4\u0da7\u0dd2\u0dad\u0dd2 \u0db6\u0ddc\u0dad\u0dca\u0dad\u0db8 \u0db8\u0ddc\u0dc4\u0ddc\u0dad\u0d9a\u0dca \u0d94\u0db6\u0db1\u0dca\u0db1."
  },
  "onboarding-6-header": {
    "message": "\u0db4\u0da7\u0dd2\u0dad\u0dd2 \u0d85\u0da9\u0d82\u0d9c\u0dd4 \u0dc3\u0db8\u0db8\u0dd4\u0dc4\u0dd6\u0dbb\u0dca\u0dad \u0db4\u0dc4\u0dc3\u0dd4\u0d9a\u0db8 \u0daf\u0dd0\u0db1\u0dca \u0dad\u0dd2\u0db6\u0dda!"
  },
  "onboarding-6-description-2": {
    "message": "\u0d94\u0db6\u0d9c\u0dda \u0d9c\u0dd2\u0dab\u0dd4\u0db8\u0da7 \u0dc3\u0db8\u0dca\u0db6\u0db1\u0dca\u0db0\u0dd2\u0dad \u0d95\u0db1\u0dd1\u0db8 \u0db4\u0dbb\u0dd2\u0d9c\u0dab\u0d9a\u0dba\u0d9a\u0dca \u0dc3\u0db8\u0d9f \u0d85\u0da9\u0d82\u0d9c\u0dd4 \u0dc3\u0dc4 \u0d85\u0da9\u0dc0\u0dd2 \u0db4\u0dd0\u0dc0\u0dbb\u0dd3\u0db8\u0dca \u0db6\u0dd9\u0daf\u0dcf \u0d9c\u0dd0\u0db1\u0dd3\u0db8\u0da7 \u0dc3\u0db8\u0db8\u0dd4\u0dc4\u0dd6\u0dbb\u0dca\u0dad\u0dba \u0dc3\u0d9a\u0dca\u200d\u0dbb\u0dd2\u0dba \u0d9a\u0dbb\u0db1\u0dca\u0db1."
  },
  "onboarding-7-header-2": {
    "message": "\u0dc3\u0db8\u0db8\u0dd4\u0dc4\u0dd6\u0dbb\u0dca\u0dad\u0dba\u0da7 \u0d9c\u0dd2\u0dab\u0dd4\u0db8\u0d9a\u0dca \u0dc0\u0dd4\u0dc0\u0db8\u0db1\u0dcf\u0dba."
  },
  "onboarding-7-description-2": {
    "message": "\u0d94\u0db6\u0d9c\u0dda \u0d9c\u0dd2\u0dab\u0dd4\u0db8 \u0dc3\u0d9a\u0dca\u200d\u0dbb\u0dd2\u0dba \u0db6\u0dc0 \u0dad\u0dc4\u0dc0\u0dd4\u0dbb\u0dd4 \u0d9a\u0dd2\u0dbb\u0dd3\u0db8\u0da7 \u0db4\u0dd2\u0dc0\u0dd2\u0dc3\u0dd9\u0db1\u0dca\u0db1 \u0d94\u0db6\u0db1\u0dca\u0db1."
  },
  "onboarding-8-description": {
    "message": "\u0daf\u0dd0\u0db1\u0dca \u0d85\u0da9\u0d82\u0d9c\u0dd4 \u0dc3\u0db8\u0d9f \u0db8\u0ddc\u0dc3\u0dd2\u0dbd\u0dca\u0dbd\u0dcf \u0dc0\u0dd3\u0db4\u0dd3\u0d91\u0db1\u0dca \u0dc4\u0ddd \u0d94\u0db6\u0d9c\u0dda\u0db8 \u0db4\u0dca\u200d\u0dbb\u0dad\u0dd2\u0dba\u0dd4\u0d9a\u0dca\u0dad\u0dba\u0d9a\u0dca \u0d92\u0d9a\u0dcf\u0db6\u0daf\u0dca\u0db0 \u0d9a\u0dbb \u0dc3\u0dd2\u0dba\u0dc5\u0dd4 \u0d9a\u0dca\u200d\u0dbb\u0dd2\u0dba\u0dcf\u0d9a\u0dcf\u0dbb\u0d9a\u0db8\u0dca \u0dc3\u0dd4\u0dbb\u0d9a\u0dca\u200d\u0dc2\u0dd2\u0dad \u0d9a\u0dbb\u0db1 \u0d86\u0dbb\u0d9a\u0dca\u200d\u0dc2\u0dd2\u0dad, \u0db4\u0dde\u0daf\u0dca\u0d9c\u0dbd\u0dd2\u0d9a \u0dc3\u0db8\u0dca\u0db6\u0db1\u0dca\u200d\u0db0\u0dad\u0dcf\u0dc0\u0d9a\u0dca \u0dc3\u0dd1\u0daf\u0dd3\u0db8\u0da7 \u0dc4\u0dd0\u0d9a\u0dd2\u0dba."
  },
